{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "starbuck": {
                "username": "$CONTAINER_REGISTRY_USERNAME_starbuck",
                "password": "$CONTAINER_REGISTRY_PASSWORD_starbuck",
                "address": "starbuck.azurecr.io"
              },
              "weatherballoon": {
                "username": "$CONTAINER_REGISTRY_USERNAME_weatherballoon",
                "password": "$CONTAINER_REGISTRY_PASSWORD_weatherballoon",
                "address": "weatherballoon.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}], \"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "DataIngestionModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.DataIngestionModule.amd64}",
              "createOptions": ""
            }
          },
          "PredictionModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.PredictionModule.amd64}",
              "createOptions": ""
            }
          },
          "LoraTransmitterModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.LoraTransmitterModule.amd64}",
              "createOptions": ""
            }
          },
          "LoraReceiverModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.LoraReceiverModule.amd64}",
              "createOptions": ""
            }
          },
          "AscentAggregationJob": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-azure-stream-analytics:1.0.0-preview010",
              "createOptions": ""
            }
          },
          "DescentAggregationJob": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "microsoft/azureiotedge-azure-stream-analytics:1.0.0-preview010",
              "createOptions": ""
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "GPSModuleToIoTHub": "FROM /messages/modules/GPSModule/outputs/* INTO $upstream",
          "IngestionModuleToIoTHub": "FROM /messages/modules/IngestionModule/outputs/* INTO $upstream",
          "DataIngestionFunctionToIoTHub": "FROM /messages/modules/DataIngestionFunction/outputs/* INTO $upstream",
          "DataIngestionModuleToIoTHub": "FROM /messages/modules/DataIngestionModule/outputs/* INTO $upstream",
          "PredictionModuleToIoTHub": "FROM /messages/modules/PredictionModule/outputs/* INTO $upstream",
          "LoraTransmitterModuleToIoTHub": "FROM /messages/modules/LoraTransmitterModule/outputs/* INTO $upstream",
          "LoraReceiverModuleToIoTHub": "FROM /messages/modules/LoraReceiverModule/outputs/* INTO $upstream",
          "AscentAggregationJobToIoTHub": "FROM /messages/modules/AscentAggregationJob/outputs/* INTO $upstream",
          "DescentAggregationJobToIoTHub": "FROM /messages/modules/DescentAggregationJob/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "AscentAggregationJob": {
      "properties.desired": {
        "ASAJobInfo": "https://edgejobs.blob.core.windows.net/ascent/ASAEdgeJobs/c2a316a9-ad8c-4c47-a87a-8acb6edf36d0/ba9e08f7-a7e3-496f-b61b-27184be4dcb9/ASAEdgeJobDefinition.zip?sv=2017-04-17&sr=b&sig=AzW4xPMBrYU0v%2FlPyKsmrtXXoVuU%2FZOInEkEFeSkHmU%3D&st=2018-09-20T23%3A34%3A32Z&se=2021-09-20T23%3A44%3A32Z&sp=r",
        "ASAJobResourceId": "/subscriptions/950d7b85-5b12-482f-a8f7-713e4bd434ea/resourceGroups/weatherballoon/providers/Microsoft.StreamAnalytics/streamingjobs/AscentAggregationJob",
        "ASAJobEtag": "67428f5a-3997-40a6-b11b-1525686ce0ab",
        "PublishTimestamp": "9/20/2018 11:44:32 PM"
      }
    },
    "DescentAggregationJob": {
      "properties.desired": {
        "ASAJobInfo": "https://edgejobs.blob.core.windows.net/descent/ASAEdgeJobs/6224bf7c-4ca2-4771-a2c5-8d1b1a43040c/efd4eff1-580f-46fa-bf5c-4672136b5bf0/ASAEdgeJobDefinition.zip?sv=2017-04-17&sr=b&sig=9Tc83xS1KqP0jGMOaojRfEV12LlZW4vkVCyA1%2BJNl5U%3D&st=2018-09-20T23%3A35%3A32Z&se=2021-09-20T23%3A45%3A32Z&sp=r",
        "ASAJobResourceId": "/subscriptions/950d7b85-5b12-482f-a8f7-713e4bd434ea/resourceGroups/weatherballoon/providers/Microsoft.StreamAnalytics/streamingjobs/DescentAggregationJob",
        "ASAJobEtag": "8ef33e8d-1fc2-43f8-a0aa-d51fec047c6c",
        "PublishTimestamp": "9/20/2018 11:45:32 PM"
      }
    }
  }
}